<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/root"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/property_color_common_home_bg"
    android:orientation="vertical" >

    <include layout="@layout/property_common_header" />

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:scrollbars="none" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingBottom="20dp"
            android:orientation="vertical" >

            <include
                android:id="@+id/baoxiu_time"
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:layout_marginTop="1dp"
                layout="@layout/property_common_text_value" />

            <include
                android:id="@+id/baoxiu_type"
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:layout_marginTop="1dp"
                layout="@layout/property_common_text_value" />

            <include
                android:id="@+id/baoxiu_problem"
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:layout_marginTop="1dp"
                layout="@layout/property_common_text_value" />

            <include
                android:id="@+id/baoxiu_picture"
                android:layout_width="match_parent"
                android:layout_height="@dimen/property_common_paishezhaoping_container_height"
                android:layout_marginTop="1dp"
                layout="@layout/property_common_display_picture" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="1dp"
                android:orientation="vertical" >

                <include
                    android:id="@+id/baoxiu_paidan_time"
                    android:layout_width="match_parent"
                    android:layout_height="40dp"
                    layout="@layout/property_common_text_value" />

                <include
                    android:id="@+id/baoxiu_paidan_weixiuren"
                    android:layout_width="match_parent"
                    android:layout_height="40dp"
                    layout="@layout/property_common_text_value" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="1dp"
                android:orientation="vertical" >

                <include
                    android:id="@+id/baoxiu_wancheng_time"
                    android:layout_width="match_parent"
                    android:layout_height="40dp"
                    layout="@layout/property_common_text_value" />

                <include
                    android:id="@+id/baoxiu_wancheng_result"
                    android:layout_width="match_parent"
                    android:layout_height="40dp"
                    layout="@layout/property_common_text_value" />

                <include
                    android:id="@+id/baoxiu_wancheng_fee"
                    android:layout_width="match_parent"
                    android:layout_height="40dp"
                    layout="@layout/property_common_text_value" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:background="@color/property_commont_color_select_type_bg"
                android:orientation="vertical" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="20dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="10dp"
                    android:gravity="center"
                    android:text="@string/property_wuyebaoxi_baoxiudan_item_yonghupingjia"
                    android:textColor="@color/property_color_common_title"
                    android:textStyle="bold" />

                <include
                    android:id="@+id/wancheng_zhiliang"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="2dp"
                    layout="@layout/property_common_pingjia" />

                <include
                    android:id="@+id/wancheng_taidu"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="2dp"
                    layout="@layout/property_common_pingjia" />

                <include
                    android:id="@+id/wancheng_jishixing"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="2dp"
                    layout="@layout/property_common_pingjia" />

                <include
                    android:id="@+id/wancheng_price"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="10dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="2dp"
                    layout="@layout/property_common_pingjia" />
            </LinearLayout>
        </LinearLayout>
    </ScrollView>

</LinearLayout>